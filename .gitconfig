[user]
  name = Kyle Mathews
  email = mathews.kyle@gmail.com
[color]
  diff = auto
  status = auto
  branch = auto
[push]
  default = tracking
[alias]
  lol = log --pretty=oneline --abbrev-commit --graph --decorate
  unstage = reset HEAD
  st = status
  ci = commit -v
  lc = log ORIG_HEAD.. --stat --no-merges
  d = diff
  dc = diff --cached
